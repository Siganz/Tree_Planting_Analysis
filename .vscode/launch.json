{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run current file (temp)",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      },
      "python": "${workspaceFolder}/.venv/Scripts/python.exe"
    }
  ]
}
